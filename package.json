{
  "name": "treblle-adonisjs",
  "version": "0.0.1",
  "description": "The official Treblle SDK for AdonisJS",
  "main": "build/providers/TreblleProvider.js",
  "engines" : {
    "npm" : ">=8.0.0",
    "node" : ">=18.0.0"
  },
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DominusKelvin/treblle-adonisjs.git"
  },
  "author": "Treblle, Dominus_Kelvin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DominusKelvin/treblle-adonisjs/issues"
  },
  "homepage": "https://github.com/DominusKelvin/treblle-adonisjs#readme",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@types/node": "^18.11.9",
    "del-cli": "^5.0.0",
    "mrm": "^4.1.13",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.8.8"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [],
    "minNodeVersion": "18.0.0",
    "probotApps": []
  },
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ]
}
